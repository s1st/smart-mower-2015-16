<?xml version="1.0" encoding="UTF-8"?>
<Params>
  <SetupManager>
    <ShowUI>true</ShowUI>
    <AutoRun>true</AutoRun>
  </SetupManager>

  <EpisodeManager>
    <EpisodeLimit> 40 </EpisodeLimit>
  </EpisodeManager>

  <StaticsticsWriter>
    <StatisticsFileName>QL_high_greed.csv</StatisticsFileName>
  </StaticsticsWriter>
  
  <Mower>
    <LearnerType>QLearner</LearnerType>
	<EligibityTraces>false</EligibityTraces>
	<Gamma>0.9</Gamma>
  </Mower>

  <QLearner>
    <Greediness>0.9</Greediness>
    <LearnRate>0.7</LearnRate>
    <InitialQValue>0.0</InitialQValue>
    <DiscountValue>1.0</DiscountValue>
  </QLearner>
</Params>
